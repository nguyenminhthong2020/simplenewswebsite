<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*@await Html.PartialAsync("_CSSPartial")*@
    <partial name="_CSSPartial" />
    @RenderSection("CSS", required: false)
</head>
<body>
    @*<header>@ViewBag.Title</header>*@
    <partial name="_HeaderPartial" />
    <section class="container-fluid sectionnav">
        <nav class="navbar navbar-expand-lg navbar-light">
            <div class="container">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        <li class="nav-item">
                            <a class="nav-link@(ViewBag.PageCode==1?" active":"")"
                               aria-current="page"
                               asp-controller="Home"
                               asp-action="Index">Trang chủ</a>

                            @*<a class="nav-link active" aria-current="page" href="#">Trang chủ</a>*@
                        </li>
                        <li class="nav-item dropdown">
                            @*<a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Danh mục
                                </a>*@
                            <a class="nav-link dropdown-toggle@(ViewBag.PageCode==2?" active":"")"
                               id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false"
                               asp-controller="Post"
                               asp-action="List">Danh mục</a>

                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li><a class="dropdown-item" href="#">Thể thao</a></li>
                                <li><a class="dropdown-item" href="#">Giáo dục</a></li>
                                <li><a class="dropdown-item" href="#">Kinh doanh</a></li>
                                <li><a class="dropdown-item" href="#">Khoa học</a></li>
                            </ul>
                        </li>
                        <a class="nav-link@(ViewBag.PageCode==3?" active":"")"
                           asp-controller="Home"
                           asp-action="Contact">Liên hệ</a>
                    </ul>
                    <form class="d-flex">
                        <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                        <button class="btn btn-outline-success mybtn" type="submit">Tìm kiếm</button>
                    </form>
                </div>
            </div>
        </nav>
    </section>
    <main>
        @RenderBody()
    </main>
    <section class="container-fluid" style="background-color: gray;">
        <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-2"><img src="~/logo/l1.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l2.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l3.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l4.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l5.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l6.png" alt="" class="d-block w-100" /></div>
                        </div>
                    </div>
                </div>
                <div class="carousel-item">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-2"><img src="~/logo/l7.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l8.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l9.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l10.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l11.png" alt="" class="d-block w-100" /></div>
                            <div class="col-2"><img src="~/logo/l12.png" alt="" class="d-block w-100" /></div>
                        </div>
                    </div>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>

        <div id="carouselExampleControls1" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active"><img src="~/img/h1.jpg" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l1.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l2.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l3.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l4.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l5.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l6.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l7.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l8.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l9.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l10.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l11.png" alt="" class="d-block w-100" /></div>
                <div class="carousel-item"><img src="~/logo/l12.png" alt="" class="d-block w-100" /></div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls1" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls1" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </section>
    <partial name="_FooterPartial" />
    <partial name="_JSPartial" />
    @*@await Html.PartialAsync("_JSPartial")*@
    @RenderSection("JS", required: false)
    <script src="~/js/layout.js"></script>
    <script src="~/js/newlayout.js"></script>
</body>
</html>
